alias run='~/net.ton.dev/scripts/run.sh'
alias check='~/net.ton.dev/scripts/check_node_sync_status.sh | grep TIME_DIFF'
alias net='cd ~/net.ton.dev/scripts'
alias tonos='cd ~/net.ton.dev/tonos-cli/target/release/ && ./tonos-cli'
