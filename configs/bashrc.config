alias operator='cd ~/operator.scripts'
alias nodelog='tail -f /var/ton-work/node.log'
alias synclog='cat ~/ton-logs/sync.log | tail -n 1000'
alias validatorlog='cat ~/ton-logs/validator.log | tail -n 100'
alias elector='cd ~/net.ton.dev/tonos-cli/target/release && ./tonos-cli runget -1:3333333333333333333333333333333333333333333333333333333333333333'
alias pscripts='cd ~/node.operator/scripts'
alias tonlogs='cd ~/ton-logs'
alias liteclient='cd ~/net.ton.dev/ton/build/lite-client'
